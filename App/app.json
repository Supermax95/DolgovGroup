{
  "expo": {
    "name": "DolgovGroup",
    "slug": "DolgovGroup",
    "version": "1.0.0",
    "orientation": "portrait",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./src/assets/images/adaptive-icon.png",
          "color": "#ffffff"
        }
      ]
    ],

    "icon": "./src/assets/images/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/adaptive-icon2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
